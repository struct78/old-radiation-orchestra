class Message {
  int bar;
  int beat;
  int click;
  String message;
  int pitch;
  int velocity;
  
  Message() {
  }
  
  Message(int _bar, int _beat, int _click, String _message, int _pitch, int _velocity) {
    bar = _bar;
    beat = _beat;
    click = _click;
    message = _message;
    pitch = _pitch;
    velocity = _velocity;
  }
}
